{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "no-underscore-dangle": ["error", { "allow": ["_id"] }],
  "rules": {
    "react/prop-types": 0
  },
  "ignorePatterns": ["node_modules", "/public/**", "/ressources/**"]
}
